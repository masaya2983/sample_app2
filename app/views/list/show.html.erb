<h1>投稿一覧</h1>
<% @lists.each do |list| %>
<h2>タイトル</h2>
<p><%= @list.title %> </p>
<h2>本文</h2>
<p><%=@list.body %></p>
<% image_tag @list, size: "200x200" %>
<% link_to "編集", edit_list_path(@list.id) %>
<% link_to "削除", destroy_list_path(@list.id),method: :delete,"date-confim"=> "本当に削除しますか？" %>
<span>
  <%= link_to list.title, "/list/#{list.id}" %>
</span>

